<configuration debug="false">

  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} %-5level %msg%n</pattern>
    </encoder>
  </appender>

  <root level="INFO">
      <appender-ref ref="STDOUT"/>
  </root>

 <logger name="arachne" level="DEBUG" />

</configuration>
